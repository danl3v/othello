(define othello-math-tester
  (list '((zero? 0) #t)
        '((zero? 1) #f)
        '((positive? 1) #t)
        '((positive? -1) #f)
        '((negative? -1) #t)
        '((negative? 1) #f)
        '((even? 2) #t)
        '((even? 1) #f)
        '((odd? 1) #f)
        '((odd? 2) #t)
        '((div 17 3) 5)
        '((div 17 -3) -5)
        '((div -17 3) -6)
        '((div -17 -3) 6)
        '((mod 17 3) 2)
        '((mod 17 -3) 2)
        '((mod -17 3) 1)
        '((mod -17 -3) 1)
        '((truncate 5.7) 5)
        '((truncate -5.7) -5)
        '((floor 5.7) 5)
        '((floor -5.7) -6)
        '((ceiling 5.7) 6)
        '((ceiling -5.7) -5)
        '((round 5.7) 6)
        '((round -5.7) -6)
        '((round 5.5) 6)
        '((round 4.5) 4)
        '((abs 4) 4)
        '((abs -4) -4)
        '((max 2 9 -4) 9)
        '((min 2 9 -4) -4)
        '((gcd 3 6 -12) 3)
        '((lcm 3 6 -9) 18)
        '((expt -2 3) -8)
        '((expt -2 -3) -1/8)
        '((expt 0 0) 1)))